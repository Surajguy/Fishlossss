run = ["npm", "start"]

modules = ["nodejs-20"]

hidden = [".config", "package-lock.json"]

[nix]
channel = "stable-24_05"

[deployment]
run = ["npm", "start"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 8000
externalPort = 80

[workflows]
runButton = "Start Fishing App"

[[workflows.workflow]]
name = "Start Fishing App"
author = 14372660
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm start"